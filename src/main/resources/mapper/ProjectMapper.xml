<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="file_management.peoject.mapper.ProjectMapper">

    <resultMap id="BaseResultMap" type="file_management.peoject.entity.Documents">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="typeId" column="type_id" jdbcType="INTEGER"/>
        <result property="fileLocation" column="file_location" jdbcType="VARCHAR"/>
        <result property="overview" column="overview" jdbcType="VARCHAR"/>
    </resultMap>


    <select id="GetProjectByTeacherID" resultType="file_management.peoject.entity.Project">
        select * from projects where teacher_id = #{teacherId}
    </select>
</mapper>
